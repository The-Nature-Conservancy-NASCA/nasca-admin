        <TabItem Header="Otros">
            <TabControl>
                <TabItem Header="Aliados">
                    <Grid>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="63*"/>
                            <ColumnDefinition Width="134*"/>
