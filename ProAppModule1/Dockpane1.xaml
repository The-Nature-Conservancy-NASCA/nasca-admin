                    <TextBox Grid.Column="0"  Text="{Binding Path=Region.File , Mode=TwoWay}" Margin="2"  ></TextBox>
                    <Button Grid.Column="1" ToolTip="Elegir archivo" Command="{Binding Path=Region.BrowseFileCommand}"  Height="20" Margin="2" HorizontalAlignment="Left" VerticalAlignment="Center">
