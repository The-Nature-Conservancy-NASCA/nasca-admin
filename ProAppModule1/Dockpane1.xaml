                    </Button>
                    <TextBox Grid.Row="0" Grid.Column="0"  Text="{Binding Path=Implementacion.File , Mode=TwoWay}" Margin="2"  ></TextBox>
                    <Button Grid.Row="0" Grid.Column="1" ToolTip="Elegir archivo" Command="{Binding Path=Implementacion.BrowseFileCommand}"  Height="20" Margin="2" HorizontalAlignment="Left" VerticalAlignment="Center">
