                        <Image Source="pack://application:,,,/ArcGIS.Desktop.Resources;component/Images/VersionPost-32.png" HorizontalAlignment="Center" VerticalAlignment="Center" ></Image>
                    </Button>
                    <TextBox Grid.Column="0"  Text="{Binding Participantes_File , Mode=TwoWay}" Margin="2"  ></TextBox>
                    <Button Grid.Column="1" ToolTip="Elegir archivo" Command="{Binding Participantes_BrowseCommand}"  Height="20" Margin="2" HorizontalAlignment="Left" VerticalAlignment="Center">
